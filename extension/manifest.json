{
  "name": "OLabel",
  "version": "1.0.0",
  "description": "Image source classification extension to improve web accessibility",
  "icons": {
    "128": "icons/icon.png"
  },
  "browser_action": {
    "default_icon": "icons/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./scripts/content.js"
      ]
    }
  ],
  "manifest_version": 2
}